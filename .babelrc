{
  "env": {
    "development": {
      "presets": [ "es2015" ]
    },
    "bootstrap-js-compile": {
      "presets": [
        [
          "es2015",
        {
          "loose": true,
          "modules": false
        }
        ]
      ],
      "plugins": [
        "transform-es2015-modules-strip"
      ]
    },
    "bootstrap-js-compile-es-plugins": {
      "presets": [
        [ "es2015", { "loose": false,  "modules": false } ]
      ],
      "plugins": []
    }
  }
}
